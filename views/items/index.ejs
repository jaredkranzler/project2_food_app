This is the admin page for editing menu



<div class="box">
  <ul>
      <% for (let i = 0; i < items.length; i++) { %>
          <li>
            
            <div class ="text">
              <h3><%=items[i].name%></h3>
            </div>

            <div class="image">
              <img src="<%=items[i].image%>">
            </div>

            <div class ="text">
              <h3><%=items[i].body%></h3>
              <h3><%=items[i].price%></h3>
            </div>

            <form>
              <input type="submit" value="Add to Cart"/>
            </form>
          </li>
      <% } %>
  </ul>
</div>



